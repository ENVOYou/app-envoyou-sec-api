{
  "name": "dash-envoyou",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:colors": "node scripts/check-hardcoded-colors.mjs",
    "lint:chrome-tokens": "node scripts/check-legacy-chrome-tokens.mjs",
    "prebuild": "pnpm lint:colors && pnpm lint:chrome-tokens",
    "daily:summary": "node scripts/generate-daily-summary.mjs",
    "daily:cleanup": "node scripts/cleanup-daily-summaries.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:visual": "pnpm exec playwright test",
    "test:visual:update": "pnpm exec playwright test --update-snapshots",
    "snapshots:grayscale": "node scripts/generate-grayscale-snapshots.mjs"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.3.31",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.2.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.17",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "sharp": "^0.34.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^2.1.4"
  },
  "packageManager": "pnpm@9.15.4"
